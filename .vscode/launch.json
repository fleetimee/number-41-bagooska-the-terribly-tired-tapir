{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}\\src\\debiturs\\entities\\debitur.entity.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"]
    },
    {
      "type": "node",
      "name": "Run Script: start:dev",
      "request": "launch",
      "command": "yarn start:dev",
      "cwd": "${workspaceFolder}"
    }
  ]
}
